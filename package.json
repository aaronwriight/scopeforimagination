{
  "name": "the-post-chaise",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 4200",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lintfix": "eslint ./components --ext .ts,.tsx --fix",
    "studio": "cd studio && yarn start"
  },
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@popmotion/popcorn": "^0.4.4",
    "@sanity/client": "^2.21.7",
    "@theme-ui/color": "^0.10.0",
    "@vercel/fetch": "^6.1.0",
    "body-scroll-lock": "^3.1.5",
    "cookie": "^0.4.1",
    "dms-conversion": "^3.1.1",
    "framer-motion": "^4.1.17",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^2.2.1",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^12.0.1",
    "next-sanity": "^0.4.0",
    "next-sanity-image": "^3.1.7",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-merge-refs": "^1.1.0",
    "react-use-measure": "^2.0.4",
    "tabbable": "^5.2.0",
    "theme-ui": "^0.10.0",
    "uuidv4": "^6.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@next/bundle-analyzer": "^10.2.3",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.8",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "deepmerge": "^4.2.2",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "groq": "^2.15.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.3.2",
    "prettier-plugin-groq": "^0.2.5",
    "typescript": "4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
